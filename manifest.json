{
  "manifest_version": 3,
  "name": "Event Filter",
  "version": "1.0",
  "description": "Filter events with score over 70% and player over 60",
  "permissions": [
    "scripting",
    "activeTab"
  ],

  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://realmstock.com/pages/event-notifier"],  
      "js": ["content.js"]
    }
  ]
}
